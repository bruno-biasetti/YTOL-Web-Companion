@charset "UTF-8";
/* CSS Document */
/*************************************/
/*** GENERAL CONFIGS AND VARIABLES ***/
/*************************************/
/*** CONFIGS ***/
.clearfix {
  width: 100%;
  height: 1px;
  display: block;
  margin: 0;
  padding: 0;
  clear: both !important;
}
/*** BODY ***/
html,
body,
div#container,
div#container-wrapper {
  min-height: 100%;
  height: auto;
}
body {
  background: #8ea9b2 url(<%= asset_path('bg/bg-main.jpg') %>) center top no-repeat scroll;
}
h1,
h2,
h3,
h4,
h5,
h6,
p,
a,
span,
label,
strong {
  font-family: 'Roboto', sans-serif;
}
.notice,
.alert {
  width: 80%;
  height: auto;
  display: block;
  position: fixed;
  top: 30%;
  left: 10%;
  z-index: 998;
  font-family: 'Roboto', sans-serif;
  text-align: center;
  margin: 0;
  padding: 1.5em 0;
  background-color: #FFF;
  border: 1px solid #999999;
  border-bottom: 4px solid #999999;
  border-radius: 10px;
  -moz-border-radius: 10px;
  -webkit-border-radius: 10px;
}
/*** CONTAINER ***/
div#container {
  width: 100%;
  height: auto;
  display: block;
  clear: both;
  margin: 0;
  padding: 0;
  padding: 2em 0 0 0;
}
.content-bg {
  background: transparent url(<%= asset_path('bg/bg-content.png') %>) left top repeat;
}
/*************************************/
/*** APPLICATION *********************/
/*************************************/
/*** LANDING ***/
div#landing-wrapper {
  width: 960px;
  height: auto;
  display: block;
  clear: both;
  margin: 0 auto;
  padding: 0;
}
div#landing-wrapper div#logo-img {
  width: 40%;
  height: auto;
  margin: 0 auto;
}
div#landing-wrapper div#logo-type {
  width: 35%;
  height: auto;
  margin: 0 auto;
}
div#landing-wrapper div#nav-enter {
  width: 40%;
  height: auto;
  margin: 1em auto;
}
div#landing-wrapper div#nav-enter ul {
  width: 100%;
  height: auto;
}
div#landing-wrapper div#nav-enter ul li {
  width: 100%;
  height: auto;
  display: block;
  padding: 0.7em 0;
  margin: 0.4em 0;
  background-color: #FFF;
  border-bottom: 4px solid #53707a;
  border-radius: 10px;
  -moz-border-radius: 10px;
  -webkit-border-radius: 10px;
  text-align: center;
}
div#landing-wrapper div#nav-enter ul li a {
  width: 30%;
  height: 22px;
  display: block;
  margin: 0 auto;
  padding: 0.1em 0 0 2em;
  text-align: center;
  color: #53707a;
  font-size: 1.2em;
  font-family: 'Roboto', sans-serif;
  font-weight: 600;
  text-decoration: none;
  text-transform: uppercase;
}
div#landing-wrapper div#nav-enter ul li.flag a {
  background: transparent url(<%= asset_path('sprite-map/sprite-flags.png') %>) left top no-repeat scroll;
}
div#landing-wrapper div#nav-enter ul li.br-flag a {
  background-position: left bottom;
}
div#landing-wrapper div#nav-enter ul li.us-flag a {
  background-position: left top;
}
/*** ACTION BAR ***/
div#action-bar {
  width: 100%;
  height: auto;
  display: block;
  clear: both;
  margin: 0;
  padding: 0;
  height: 2.5em !important;
  padding: 0.5em 0 !important;
  overflow: visible;
  position: fixed;
  z-index: 999;
  background-color: #dddddd;
  border-bottom: 4px solid #d2d2d2;
  box-shadow: #bcbebe 0px 2px 2px;
  -moz-box-shadow: #bcbebe 0px 2px 2px;
  -webkit-box-shadow: #bcbebe 0px 2px 2px;
}
div#action-bar ul {
  width: 960px;
  height: auto;
  display: block;
  clear: both;
  margin: 0 auto;
  padding: 0;
  overflow: visible;
}
div#action-bar ul li {
  display: block;
  float: left;
  margin-right: 1em;
}
div#action-bar ul li#call-nav-drawer,
div#action-bar ul li#call-action-nav {
  margin-top: 0.5em;
}
div#action-bar ul li#call-nav-drawer {
  float: left;
}
div#action-bar ul li#call-action-nav {
  float: right;
}
div#action-bar ul li#call-nav-drawer a,
div#action-bar ul li#call-action-nav a {
  background: transparent url(<%= asset_path('sprite-map/sprite-action-bar-navs.png') %>) left top no-repeat scroll;
  text-indent: -3000px;
  display: block;
  height: 23px;
  width: 1.1em;
}
div#action-bar ul li#call-nav-drawer a {
  background-position: left top;
}
div#action-bar ul li#call-action-nav a {
  background-position: right bottom;
}
div#action-bar ul li#icon-title {
  overflow: hidden;
  width: 50%;
  margin-left: 3%;
}
div#action-bar ul li#icon-title img,
div#action-bar ul li#icon-title h2 {
  float: left;
}
div#action-bar ul li#icon-title h2 {
  font-family: 'Roboto', sans-serif;
  font-size: 1.5em;
  font-weight: 200;
  color: #FFF;
  margin: 0.3em 0 0 0.5em;
}
/*** LOAD CLICK ***/
div#load-click {
  width: 100%;
  height: 100%;
  display: none;
  position: fixed;
  overflow: hidden;
  z-index: 998;
}
div#load-click #load-container {
  width: 50px;
  height: 50px;
  display: block;
  position: fixed;
  top: 50%;
  left: 50%;
  margin: -25px 0px 0px -25px;
  padding: 0;
  background-color: #FFF;
  border-radius: 10px;
  z-index: 999;
}
div#load-click #load-container img {
  width: 16px;
  height: 16px;
  display: block;
  margin: 1em auto;
}
div#load-click #load-layer {
  width: 100%;
  height: 100%;
  display: block;
  position: fixed;
  overflow: hidden;
  background-color: #000;
  opacity: 0.7;
  -moz-opacity: 0.7;
  filter: alpha(opacity=70);
  z-index: 998;
}
/*** NAVIGATION DRAWER ***/
div#nav-drawer {
  width: 100%;
  height: auto;
  display: block;
  clear: both;
  margin: 0;
  padding: 0;
  height: 100% !important;
  position: absolute;
  z-index: 990;
}
div#nav-drawer ul#nav-drawer-container {
  width: 50%;
  height: 100% !important;
  display: block;
  background-color: #f5f5f5;
  margin: 0;
  padding-top: 5em;
  position: fixed;
  z-index: 992;
  top: 0;
  left: 0;
}
div#nav-drawer ul#nav-drawer-container li,
div#nav-drawer ul#nav-drawer-container a,
div#nav-drawer ul#nav-drawer-container span {
  color: #7d7c7c;
}
div#nav-drawer ul#nav-drawer-container a {
  text-decoration: none;
}
div#nav-drawer ul#nav-drawer-container a.arrow-ctrl {
  width: 48px;
  height: 7px;
  display: block;
  float: right;
  margin: 0.5em 0 0 0;
  background: url(<%= asset_path('sprite-map/sprite-nav-draw-arrow.png') %>) left top no-repeat scroll;
}
div#nav-drawer ul#nav-drawer-container a.arrow-down {
  background-position: left bottom;
}
div#nav-drawer ul#nav-drawer-container a.arrow-up {
  background-position: left top;
}
div#nav-drawer ul#nav-drawer-container li {
  padding: 1em 0 1em 1em;
  margin: 0;
  border-bottom: 1px solid #dddddd;
  clear: both;
}
div#nav-drawer ul#nav-drawer-container ul.closed {
  display: none;
}
div#nav-drawer ul#nav-drawer-container ul.open {
  display: block;
}
div#nav-drawer ul#nav-drawer-container ul.first-level,
div#nav-drawer ul#nav-drawer-container ul.second-level {
  margin-top: 1em;
}
div#nav-drawer ul#nav-drawer-container ul.first-level {
  width: 100%;
  background-color: #e7e8e9;
  margin-left: -1em;
  padding-left: 1em;
}
div#nav-drawer ul#nav-drawer-container ul.first-level li {
  border-bottom: 1px solid #d7d8d9;
}
div#nav-drawer ul#nav-drawer-container ul.first-level li,
div#nav-drawer ul#nav-drawer-container ul.first-level a,
div#nav-drawer ul#nav-drawer-container ul.first-level span {
  color: #3a3a3c;
}
div#nav-drawer ul#nav-drawer-container ul.second-level {
  width: 100%;
  background-color: #c5c6c7;
  margin-left: -2em;
  padding-right: 2em;
}
div#nav-drawer ul#nav-drawer-container ul.second-level li {
  border-bottom: 1px solid #b6b7b7;
  padding-left: 4em;
}
div#nav-drawer ul#nav-drawer-container ul.second-level li,
div#nav-drawer ul#nav-drawer-container ul.second-level a,
div#nav-drawer ul#nav-drawer-container ul.second-level span {
  color: #FFF;
}
div#nav-drawer div#nav-layer {
  width: 100%;
  height: 100%;
  position: fixed;
  z-index: 991;
  background-color: #333;
  opacity: 0.7;
  -moz-opacity: 0.7;
  filter: alpha(opacity=70);
}
/*** CONTENT CONTAINER ***/
div#container-wrapper {
  width: 960px;
  height: auto;
  display: block;
  clear: both;
  margin: 0 auto;
  padding: 0;
  padding: 2em 0 0 0;
}
/*** SIGN IN / SIGN UP ***/
div#register-header {
  width: 100%;
  height: auto;
  display: block;
  margin: 0;
  padding: 0;
  height: 8em;
  border-bottom: 1px solid #cccaca;
  clear: both;
}
div#register-header div#register-header-wrapper {
  width: 26.5em;
  height: 7em;
  display: block;
  margin: 0 auto;
}
div#register-header div#register-header-wrapper div#register-sign-in,
div#register-header div#register-header-wrapper div#register-sign-up {
  height: 100%;
  display: block;
  float: left;
  padding: 0.5em 4em;
  margin: 0 auto;
}
div#register-header div#register-header-wrapper div#register-sign-in h3,
div#register-header div#register-header-wrapper div#register-sign-up h3 {
  width: 100%;
  text-align: center;
  text-decoration: none;
  font-family: 'Roboto', sans-serif;
  font-size: 1.2em;
  font-weight: 400;
  color: #c36b32;
}
div#register-header div#register-header-wrapper div#register-sign-in a,
div#register-header div#register-header-wrapper div#register-sign-up a {
  text-decoration: none;
  color: #c36b32;
}
div#register-header div#register-header-wrapper div#register-sign-in {
  border-right: 1px solid #cccaca;
}
div#register-header div#register-header-wrapper div#register-sign-up {
  border-left: 1px solid #ffffff;
}
div#register-header div#register-header-wrapper span.register-icon {
  width: 55px;
  height: 55px;
  display: block;
  margin: 0.5em auto;
  background: transparent url(<%= asset_path('sprite-map/spirte-round-icons.png') %>) left top no-repeat scroll;
}
div#register-header div#register-header-wrapper span.icon-signin-on {
  background-position: left top;
}
div#register-header div#register-header-wrapper span.icon-signin-off {
  background-position: right top;
}
div#register-header div#register-header-wrapper span.icon-signup-on {
  background-position: left -65px;
}
div#register-header div#register-header-wrapper span.icon-signup-off {
  background-position: right -65px;
}
div#register-container {
  width: 100%;
  height: auto;
  display: block;
  clear: both;
  margin: 0;
  padding: 0;
  border-top: 1px solid #FFF;
  padding: 2em 0;
}
div#register-container div#devise-shared-links a {
  width: 50%;
  height: auto;
  display: block;
  margin: 0 auto;
  padding: 0.4em 0;
  font-size: 0.8em;
  text-decoration: none;
  color: #53707a;
  background-color: #FFF;
  border-top: 1px solid #53707a;
  border-left: 1px solid #53707a;
  border-right: 1px solid #53707a;
  border-bottom: 4px solid #53707a;
  border-radius: 10px;
  -moz-border-radius: 10px;
  -webkit-border-radius: 10px;
  text-align: center;
}
/*** INPUTS ***/
div.input {
  width: 100%;
  height: auto;
  display: block;
  clear: both;
  margin: 0;
  padding: 0;
  margin-bottom: 1em !important;
}
div.input label {
  font-family: 'Roboto', sans-serif;
  font-size: 1.1em;
  font-weight: 300;
  color: #999b9b;
  display: block;
  clear: both;
  width: auto;
  margin: 0.7em 0;
}
div.input input.string,
div.input input.password,
div.input textarea {
  width: 98% !important;
  margin: 0;
  padding: 0.2em 1%;
  background-color: transparent;
  background-image: none;
  border-top: none;
  border-left: 1px solid #a7aaaa;
  border-right: 1px solid #a7aaaa;
  border-bottom: 1px solid #a7aaaa;
  font-family: 'Roboto', sans-serif;
  font-size: 1.2em;
  color: #999b9b;
}
div.input input.boolean {
  width: 2em;
  height: 2em;
}
div.input input:focus {
  border-left: 2px solid #33b5e5;
  border-right: 2px solid #33b5e5;
  border-bottom: 2px solid #33b5e5;
}
div.form-actions {
  padding: 1em 0;
  margin: 0 0 1em 0;
}
div.form-actions input.action-send {
  width: 60px;
  height: 60px;
  display: block;
  margin: 0 auto;
  padding: 0;
  border: none !important;
  text-indent: -3000px;
  cursor: pointer;
  background: transparent url(<%= asset_path('sprite-map/spirte-round-icons.png') %>) left -130px no-repeat scroll;
}
/*** APP INIT ***/
div#init-container {
  width: 100%;
  height: auto;
  display: block;
  clear: both;
  margin: 0;
  padding: 0;
}
div#init-container ul,
div#init-container li {
  width: 100%;
  height: auto;
  display: block;
  clear: both;
  margin: 0;
  padding: 0;
}
div#init-container li.init-link {
  padding: 3.5em 0px;
  border-bottom: 1px solid #FFF;
  background-color: transparent;
  text-align: center;
  margin-bottom: 0.2em;
  position: relative;
}
div#init-container li.init-link a {
  font-family: 'Roboto', sans-serif;
  font-weight: 400;
  font-size: 1.5em;
  color: #c36b32;
  text-align: center;
  text-decoration: none;
}
div#init-container li.init-link span.init-icon {
  width: 94px;
  height: 70px;
  display: block;
  position: absolute;
  top: 25%;
  left: 27%;
  background: transparent url(<%= asset_path('sprite-map/sprite-init-page.png') %>) left top no-repeat scroll;
}
div#init-container li.movement-link span.init-icon {
  background-position: left top;
}
div#init-container li.guide-link span.init-icon {
  background-position: left -70px;
}
div#init-container li.contact-link span.init-icon {
  background-position: left -140px;
}
div#init-container li.maria-link {
  background-color: #FFF;
}
div#init-container li.maria-link span.init-icon {
  top: 20%;
  height: 85px;
  background-position: left -210px;
}
/*** PAGE CONTENT ***/
.content-container {
  width: 100%;
  height: auto;
  display: block;
  clear: both;
  margin: 0;
  padding: 0;
}
.content-container .content-header {
  width: 100%;
  height: auto;
  display: block;
  clear: both;
  margin: 0;
  padding: 0;
  padding: 1.5em 0;
  border-bottom: 1px solid #cccaca;
}
.content-container .content-header span {
  width: 94px;
  height: 64px;
  display: block;
  margin: 0 auto;
  clear: both;
  background: transparent url(<%= asset_path('sprite-map/sprite-header-icons.png') %>) center top no-repeat scroll;
}
.content-container .content-header h2 {
  width: 100%;
  height: auto;
  margin-top: 0.7em;
  color: #c36b32;
  text-align: center;
  font-size: 1.7em;
  font-weight: 500;
}
.content-container #movement-header span {
  background-position: -10px -10px;
}
.content-container #guide-header span {
  background-position: -144px -10px;
}
.content-container #prayers-header span {
  background-position: -10px -110px;
}
.content-container #magnificat-header span {
  background-position: -10px -215px;
}
.content-container #timeout-header span {
  background-position: -10px -320px;
}
.content-container #mary-header span {
  background-position: -10px -215px;
}
.content-container #spirit-header span {
  background-position: -10px -530px;
}
.content-container #father-header span {
  background-position: -10px -630px;
}
.content-container #rosary-header span,
.content-container #mistery-glorious-header span,
.content-container #mistery-joyful-header span,
.content-container #mistery-light-header span,
.content-container #mistery-sorrowful-header span {
  background-position: -10px -420px;
}
.content-container #reflections-header span,
.content-container #reflections-new-header span,
.content-container #reflections-edit-header span {
  background-position: -144px -110px;
}
.content-container #sharings-header span,
.content-container #sharings-new-header span,
.content-container #sharings-show-header span,
.content-container #sharings-edit-header span {
  background-position: -144px -430px;
}
.content-container #efforts-header span,
.content-container #efforts-new-header span,
.content-container #efforts-show-header span,
.content-container #efforts-edit-header span {
  background-position: -144px -430px;
}
.content-container #contact-header span {
  background-position: -144px -220px;
}
.content-container #maria-header span {
  background-position: -144px -430px;
}
.content-container #maria-video-header span {
  background-position: -144px -630px;
}
.content-container #maria-music-header span {
  background-position: -144px -530px;
}
.content-container .content-body {
  width: 100%;
  height: auto;
  display: block;
  clear: both;
  margin: 0;
  padding: 0;
  border-top: 1px solid #FFF;
  padding-top: 0 !important;
}
.content-container .content-body p {
  width: 96%;
  height: auto;
  display: block;
  margin: 0 auto;
  padding: 0.5em 2%;
  text-align: justify;
  font-size: 1.1em;
  color: #525354;
  line-height: 1.3em;
}
.content-container .content-body .show {
  width: 96%;
  height: auto;
  min-height: auto;
  max-height: 35em;
  display: block;
  margin: 1em auto;
  padding: 0.5em 2%;
  overflow-y: scroll;
  text-align: justify;
  font-size: 1.1em;
  color: #525354;
  line-height: 1.3em;
}
.content-container .content-body .show-quote {
  font-style: italic;
}
.content-container .content-body .show-big {
  text-align: left !important;
}
.content-container .content-body .response-block {
  display: block;
  width: 96%;
  height: auto;
  padding: 0.5em 2%;
  margin: 0.5em 0 0 0;
  background-color: #DDD;
  border-bottom: 2px solid #AAA;
}
.content-container .content-body .content-list {
  width: 100%;
  height: auto;
  display: block;
  clear: both;
  margin: 0;
  padding: 0;
}
.content-container .content-body .content-list .content-list-item {
  width: 100%;
  height: auto;
  display: block;
  margin: 0;
  padding: 0;
  border-bottom: 1px solid #cccaca;
  border-top: 1px solid #FFF;
  color: #c36b32;
  font-weight: 400;
  text-decoration: none;
  padding: 1.5em 0 !important;
  overflow: hidden;
  clear: both;
}
.content-container .content-body .content-list .content-list-item a {
  color: #c36b32;
  font-weight: 400;
  font-size: 1.2em;
  text-decoration: none;
  display: block;
  width: 65%;
  float: left;
  padding: 0.5em 0;
  margin: 0.4em 0 0.4em 10%;
  background: transparent url(<%= asset_path('bg/bg-arrow.png') %>) right 10px no-repeat scroll;
}
.content-container .content-body .content-list .content-list-item span.list-item-icon {
  width: 55px;
  height: 55px;
  display: block;
  margin: 0 0 0 9%;
  padding: 0;
  float: left;
  background: transparent url(<%= asset_path('sprite-map/sprite-list-icons.png') %>) left top no-repeat scroll;	
}
.content-container .content-body .content-list .content-list-item span.prayer-icon {
  background-position: -10px -10px;
}
.content-container .content-body .content-list .content-list-item span.reflection-icon {
  background-position: -95px -10px;
}
.content-container .content-body .content-list .content-list-item span.sharing-icon {
  background-position: -180px -10px;
}
.content-container .content-body .content-list .content-list-item span.effort-icon {
  background-position: -265px -10px;
}
.content-container .content-body .content-list .content-list-item span.mary-icon {
  background-position: -350px -10px;
}
.content-container .content-body .content-list .content-list-item span.spirit-icon {
  background-position: -435px -10px;
}
.content-container .content-body .content-list .content-list-item span.cross-icon {
  background-position: -520px -10px;
}
.content-container .content-body .content-list .content-list-item span.time-icon {
  background-position: -10px -95px;
}
.content-container .content-body .content-list .content-list-item span.rosary-icon {
  background-position: -95px -95px;
}
.content-container .content-body .content-list .content-list-item span.web-icon {
  background-position: -265px -95px;
}
.content-container .content-body .content-list .content-list-item span.fb-icon {
  background-position: -350px -95px;
}
.content-container .content-body .content-list .content-list-item span.mail-icon {
  background-position: -435px -95px;
}
.content-container .content-body .content-list .content-list-item span.video-icon {
  background-position: -10px -180px;
}
.content-container .content-body .content-list .content-list-item span.song-icon {
  background-position: -520px -95px;
}
.content-container .content-body .content-list .content-list-item .circle-icon {
  width: 28px;
  height: 28px;
  display: block;
  float: left;
